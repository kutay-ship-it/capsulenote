{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "DATABASE_URL",
    "NEXT_PUBLIC_APP_URL",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_VAPID_PUBLIC_KEY",
    "NEXT_PUBLIC_SENTRY_DSN",
    "CLERK_SECRET_KEY",
    "CLERK_WEBHOOK_SECRET",
    "CLERK_AUTO_PROVISION_ENABLED",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "STRIPE_PRICE_DIGITAL_ANNUAL",
    "STRIPE_PRICE_PAPER_ANNUAL",
    "STRIPE_PRICE_ADDON_EMAIL",
    "STRIPE_PRICE_ADDON_PHYSICAL",
    "STRIPE_PRICE_TRIAL_PHYSICAL",
    "RESEND_API_KEY",
    "RESEND_WEBHOOK_SECRET",
    "EMAIL_FROM_NOTIFICATION",
    "EMAIL_FROM_DELIVERY",
    "EMAIL_FROM",
    "INNGEST_SIGNING_KEY",
    "INNGEST_EVENT_KEY",
    "UPSTASH_REDIS_REST_URL",
    "UPSTASH_REDIS_REST_TOKEN",
    "LOB_API_KEY",
    "LOB_WEBHOOK_SECRET",
    "LOB_TEMPLATE_ID",
    "LOB_TEMPLATE_VERSION_ID",
    "LOB_LIVE_API_KEY",
    "LOB_LIVE_PUB_KEY",
    "LOB_TEST_API_KEY",
    "LOB_TEST_PUB_KEY",
    "CLICKSEND_USERNAME",
    "CLICKSEND_API_KEY",
    "POSTMARK_SERVER_TOKEN",
    "CRYPTO_MASTER_KEY",
    "CRON_SECRET",
    "UNLEASH_API_URL",
    "UNLEASH_API_TOKEN",
    "UNLEASH_APP_NAME",
    "OTEL_EXPORTER_OTLP_ENDPOINT",
    "OTEL_EXPORTER_OTLP_HEADERS",
    "SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "DEV_BYPASS_SUBSCRIPTION",
    "VAPID_PRIVATE_KEY",
    "VAPID_MAILTO"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}

{
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "crons": [
    {
      "path": "/api/cron/rollover-usage",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/reconcile-deliveries",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/cleanup-pending-subscriptions",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/cleanup-expired-drafts",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/cleanup-anonymous-drafts",
      "schedule": "0 4 * * *"
    },
    {
      "path": "/api/cron/reconcile-webhooks",
      "schedule": "*/5 * * * *"
    }
  ]
}
